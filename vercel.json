{
  "buildCommand": "cd client && ./node_modules/.bin/vite build",
  "installCommand": "npm install && cd client && npm install --include=dev",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
