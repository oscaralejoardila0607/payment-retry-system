{
  "buildCommand": "cd client && npm run build",
  "installCommand": "npm install && cd client && npm install",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
